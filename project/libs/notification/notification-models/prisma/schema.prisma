generator client {
  provider = "prisma-client-js"
  output   = "../../../../node_modules/.prisma/notification-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Subscriber {
  id       String @id @default(uuid()) @map("id")
  email    String @unique
  nickname String

  @@map("subscribers")
}
